{
  "name": "@tapsellorg/eslint-plugin",
  "version": "2.0.4",
  "description": "An Eslint plugin recommended by Tapsell.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "author": "Vahid Mohammadi <vahidm1467@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tapsellorg/eslint-plugin/issues"
  },
  "homepage": "https://github.com/tapsellorg/eslint-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapsellorg/eslint-plugin.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -b tsconfig.build.json",
    "prepare": "npm run build",
    "clean": "tsc -b tsconfig.build.json --clean",
    "start": "tsc --watch",
    "test": "mocha dist/tests --recursive --require esm --watch",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "requireindex": "~1.1.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-rxjs": "0.0.3-beta.30",
    "eslint-plugin-unicorn": "^23.0.0",
    "esm": "^3.2.25",
    "generator-eslint": "^2.0.0",
    "mocha": "^7.2.0",
    "prettier": "^2.1.2",
    "yo": "^3.1.1"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
