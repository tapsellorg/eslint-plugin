  "devDependencies": {
    "eslint": "^7.1.0",
    "esm": "^3.2.25",
    "generator-eslint": "^2.0.0",
    "mocha": "^7.2.0",
    "prettier": "^2.1.2",
    "yo": "^3.1.1"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^4.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
