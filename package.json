{
  "name": "@tapsellorg/eslint-plugin",
  "version": "4.2.0",
  "description": "An Eslint plugin recommended by Tapsell.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "author": "Vahid Mohammadi <vahidm1467@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tapsellorg/eslint-plugin/issues"
  },
  "homepage": "https://github.com/tapsellorg/eslint-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapsellorg/eslint-plugin.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rm -rf dist",
    "build": "tsc -b tsconfig.build.json",
    "clean": "tsc -b tsconfig.build.json --clean",
    "start": "tsc --watch",
    "test": "mocha dist/tests --recursive --require esm --timeout 15000",
    "postversion": "git push && git push --tags",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular-eslint/builder": "^12.3.1",
    "@angular-eslint/eslint-plugin": "^12.3.1",
    "@angular-eslint/eslint-plugin-template": "^12.3.1",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "^12.3.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-rxjs": "3.3.7",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "prettier": "^2.3.2",
    "requireindex": "~1.2.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/prompt": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "esm": "^3.2.25",
    "generator-eslint": "^2.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "mocha": "^8.2.1",
    "yo": "^3.1.1"
  },
  "peerDependencies": {
    "eslint": "^7.32.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
