{
  "name": "@tapsellorg/eslint-config",
  "version": "2.0.1",
  "description": "An Eslint plugin recommended by Tapsell.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "author": "Vahid Mohammadi <vahidm1467@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tapsellorg/eslint-config/issues"
  },
  "homepage": "https://github.com/tapsellorg/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapsellorg/eslint-config.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -b tsconfig.build.json",
    "prepare": "npm run build",
    "clean": "tsc -b tsconfig.build.json --clean",
    "start": "tsc --watch",
    "test": "mocha dist/tests --recursive --require esm --watch"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "requireindex": "~1.1.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "eslint": "^7.1.0",
    "esm": "^3.2.25",
    "generator-eslint": "^2.0.0",
    "mocha": "^7.2.0",
    "prettier": "^2.1.2",
    "yo": "^3.1.1"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^4.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
