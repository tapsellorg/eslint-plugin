{
  "name": "@tapsellorg/eslint-plugin",
  "version": "4.2.7",
  "description": "An Eslint plugin recommended by Tapsell.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "author": "Vahid Mohammadi <vahidm1467@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tapsellorg/eslint-plugin/issues"
  },
  "homepage": "https://github.com/tapsellorg/eslint-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapsellorg/eslint-plugin.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rm -rf dist",
    "build": "tsc -b tsconfig.build.json",
    "clean": "tsc -b tsconfig.build.json --clean",
    "start": "tsc --watch",
    "test": "mocha dist/tests --recursive --require esm --timeout 15000",
    "postversion": "git push && git push --tags",
    "prepare": "husky install"
  },
  "dependencies": {
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "prettier": "^2.5.1",
    "requireindex": "~1.2.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/prompt": "^15.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "esm": "^3.2.25",
    "generator-eslint": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "mocha": "^9.1.3",
    "yo": "^3.1.1"
  },
  "peerDependencies": {
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-rxjs": "^4.0.3",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unused-imports": "^2.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
